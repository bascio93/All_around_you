<li id="Domande-<%= domande.id %>">
  <span class="nome"><%= link_to "Visualizza domanda", domande_path(domande.id)%></span>
  <span class="nome"><%= link_to domande.user.name, domande.user%></span>
  <span class="nome"><%= link_to domande.servizi.nome, domande.servizi%></span>
  <span class="content"><%= domande.content %></span>

  <span class="timestamp">
  <% if currentuser==domande.user %>
      <%= link_to "Cancella", cancelladomande_path(domande.id) %>
    <% end %>
  </span>
</li>
